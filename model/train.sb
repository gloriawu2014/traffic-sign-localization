#!/bin/bash
#SBATCH --job-name=train
#SBATCH --partition=gpu-a5000-q
#SBATCH --gres=gpu:1
#SBATCH --cpus-per-task=4
#SBATCH --mem=16G
#SBATCH --time=10:00:00
#SBATCH --output=train.out
#SBATCH --error=train.err

#sbatch train.sb

EPOCH=${1:-25}
LR=${2:-0.0025}

cd ../
source .venv/bin/activate
cd model
python3 train.py --epochs $EPOCH --lr $LR
